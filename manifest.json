{
  "manifest_version": 2,

  "name": "No Click Bait Plugin",
  "description": "This extension will give you information without clicking",
  "version": "1.0",

  "browser_action": {
    "default_icon": "./icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.ynet.co.il/*"],
      "js": ["./jquery.min.js", "./js/main.js"]
    }
  ]
}
